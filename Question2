object Add{
  def main (args: Array[String]):Unit={
    var k,i,j:Int=2;
    var m,n:Int=5;
    var f:Float=12.0f;
    var g:Float=4.0f;
    var c:Char='X';

    println(k+12*m);    //62
    println(m/j);       //2
    println(n%j);       //1
    println(m/j*j);     //4
    println(f+10*5+g);  //66.0
    i=i+1;  
    println(i*n);       //15

  }

}
