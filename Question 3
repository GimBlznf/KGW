object Add{
  def main (args: Array[String]):Unit={
    var a:Int=2;
    var b:Int=3;
    var c:Int=4;
    var d:Int=5;
    var k:Float=4.3f;
    var g:Float=4.0f
    
    println( (b-1) * a + c *(d)); //24
    println(a+1);                 //3
    println (-2 * ( g - k ) +c);  //4.6000004
    println (c==c);               //true
    println ((c+1)==(c+2)*(a+1)); //false

    
  }

}
